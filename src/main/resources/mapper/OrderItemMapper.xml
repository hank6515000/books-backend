<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.booksbackend.mapper.OrderItemMapper">

    <resultMap id="BaseResultMap" type="com.example.booksbackend.pojo.OrderItem">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="book" column="book" jdbcType="INTEGER"/>
            <result property="buycount" column="buyCount" jdbcType="INTEGER"/>
            <result property="orderbean" column="orderBean" jdbcType="INTEGER"/>
            <result property="orderno" column="orderNo" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,book,buyCount,
        orderBean,orderNo
    </sql>
</mapper>
